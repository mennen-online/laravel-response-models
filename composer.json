{
  "name": "mennen-online/laravel-http-response-processor",
  "description": "Convert Http JSON Responses in standard JSON Format",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0.0",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "MennenOnline\\LaravelHttpResponseProcessor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MennenOnline\\LaravelHttpResponseProcessor\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  }
}
